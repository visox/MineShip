class ComputeColorLerpAlpha : ComputeColor
{
    ComputeColor color1;
    ComputeColor color2;

    override float4 Compute()
    {
        float4 tex1 = color1.Compute();
        float4 tex2 = color2.Compute();

        float3 mix1 = lerp(tex1.rgb, tex2.rgb, tex2.a);

        return float4(mix1, 1.0f);
    }
};
